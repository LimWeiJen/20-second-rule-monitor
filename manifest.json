{
	"manifest_version": 2,
	"name": "20SecondRuleMonitor",
	"version": "1.0",
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["Content.js"]
		}
	],
	"browser_action": {
		"default_popup": "Popup.html",
		"default_title": "20 Second Rule Monitor"
	},
	"permissions": [
		"storage"
	]
}